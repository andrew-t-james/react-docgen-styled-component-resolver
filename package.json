{
  "name": "react-docgen-styled-component-resolver",
  "version": "1.0.3",
  "main": "index.js",
  "author": "Andrew T. James",
  "license": "MIT",
  "repository": "https://github.com/andrew-t-james/react-docgen-styled-component-resolver/",
  "files": [
    "index.js"
  ],
  "keywords": [
    "react-docgen",
    "resolver",
    "styled",
    "styled-components",
    "documentation"
  ],
  "scripts": {
    "example": "node example-usage.js",
    "lint": "eslint '*.js'",
    "prettier": "prettier --write '*.js'",
    "test": "jest --verbose",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "jest --watch --verbose"
  },
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/parser": "^7.11.5",
    "ast-types": "0.13.2",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "react-docgen": "5.3.0"
  },
  "peerDependencies": {
    "react-docgen": ">= 5.0.0"
  }
}
